<?xml version="1.0" encoding="utf-8" ?>
<component name="CustomRowListItem" extends="Group">
  <script type="text/brightscript" uri="CustomRowListItem.brs" />
  <interface>
    <field id="width" type="integer" onChange="onWidthChanged" />
    <field id="height" type="integer" onChange="onHeightChanged" />
  </interface>
  <children>
    <Rectangle 
        id="background" 
        color="#1BD379" 
        opacity="0.8" 
        width="150"
        height="100"/>
  </children>
</component>
