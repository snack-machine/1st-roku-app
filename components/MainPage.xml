<?xml version="1.0" encoding="utf-8" ?>
<component name="MainPage" extends="Group">
  <script type="text/brightscript" uri="MainPage.brs" />
  <interface>
    <field id="backgroundColor" type="color" alias="rectangle.color" />
  </interface>
  <children>
    <Rectangle id="rectangle" width="1920" height="1080" color="#ECEFF1" translation="[0,0]" />
    <RowList 
      id="mainRowList" 
      translation="[50,50]" 
      rowItemSize="[[200,200]]"
      itemSize="[1920,200]"
      itemSpacing="[0,0]"
      itemComponentName="CustomRowListItem"
      vertFocusAnimationStyle="fixedFocusWrap" 
      rowFocusAnimationStyle = "floatingFocus"
      numRows="99" />
  </children>
</component>
